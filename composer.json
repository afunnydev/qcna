{
  "require": {
    "aelvan/imager": "v2.4.0",
    "barrelstrength/sprout-active": "2.2.0",
    "barrelstrength/sprout-reports": "1.5.10",
    "craftcms/cms": "3.7.67",
    "craftcms/contact-form": "2.2.7",
    "craftcms/feed-me": "4.4.0",
    "craftcms/redactor": "2.8.8",
    "dukt/analytics": "4.0.10",
    "ether/seo": "3.7.4",
    "linslin/yii2-curl": "^1.3",
    "misterbk/mix": "1.5.2",
    "sebastianlenz/linkfield": "1.0.25",
    "solspace/craft-express-forms": "1.1.1",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "verbb/cp-nav": "3.0.17",
    "verbb/field-manager": "2.2.4",
    "verbb/super-table": "2.6.8",
    "vlucas/phpdotenv": "^3.4.0",
    "wbrowar/craft-communicator": "1.0.2"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
